{
    "engines": {
        "node": ">=20.0.0"
    },
    "scripts": {
        "start": "node ./build/",
        "start:tiny-chat": "npx babel-node server/tiny-chat/index.js",
        "build:tiny-chat": "node build/pkg/tiny-chat.js",
        "build:tiny-chat:all": "node build/pkg/tiny-chat.js --os=all",
        "build:tiny-chat:win32": "node build/pkg/tiny-chat.js --os=win32",
        "build:tiny-chat:linux": "node build/pkg/tiny-chat.js --os=linux",
        "build:tiny-chat:darwin": "node build/pkg/tiny-chat.js --os=darwin",
        "example:tiny-chat-timedmap": "npx babel-node server/tiny-chat/old/examples/TimedMap.js",
        "install": "yarn build-bundle",
        "build-bundle": "browserify build/bundle/require -o docs/bundle.js && browserify build/bundle/import -p esmify --ignore fs --ignore fs/promises > docs/bundle2.js && npx babel-node build/bundle/css && npx babel-node build/bundle/files",
        "fix:prettier": "prettier --write ./docs/js/chapter_manager/* && prettier --write ./docs/js/files/* && prettier --write ./docs/js/ai/* && prettier --write ./docs/js/pwa/* && prettier --write ./server/tiny-chat/*",
        "update-base64": "node ./build/base64",
        "update-chapter": "node ./build/chapter",
        "update-characters": "node ./build/characters",
        "update-sitemap": "node ./build/sitemap",
        "deploy-keybase": "node ./build/deploy-keybase"
    },
    "author": {
        "name": "JasminDreasond (Yasmin Seidel)",
        "email": "tiny@puddy.club",
        "url": "https://github.com/JasminDreasond"
    },
    "pkg": {
        "assets": "dist/server/tiny-chat/config.ini"
    },
    "dependencies": {
        "@cryptofonts/cryptofont": "^1.3.5",
        "@fortawesome/fontawesome-free": "^6.7.2",
        "@json-editor/json-editor": "^2.15.2",
        "@popperjs/core": "^2.11.8",
        "bootstrap": "^5.3.7",
        "buffer": "^6.0.3",
        "clone": "^2.1.2",
        "events": "^3.3.0",
        "exifreader": "^4.31.1",
        "express": "^5.1.0",
        "file-saver": "^2.0.5",
        "for-promise": "^2.0.4",
        "gasparesganga-jquery-loading-overlay": "^2.1.7",
        "glob": "^11.0.3",
        "ini": "^5.0.0",
        "jquery": "^3.7.1",
        "js-base64": "^3.7.7",
        "js-loading-overlay": "^1.2.0",
        "jsstore": "^4.9.0",
        "marked": "^16.0.0",
        "md5": "^2.3.0",
        "moment": "^2.30.1",
        "object-hash": "^3.0.0",
        "paginate-array": "^2.1.0",
        "photoswipe": "^5.4.4",
        "pizzicato": "^0.6.4",
        "puddysql": "^1.0.0-beta",
        "qrcode": "^1.5.0",
        "sitemap": "^8.0.0",
        "socket.io": "^4.8.1",
        "socket.io-client": "^4.8.1",
        "tiny-ai-api": "^1.1.6",
        "tiny-dices": "^1.0.6",
        "tiny-essentials": "^1.18.1",
        "tiny-server-essentials": "^1.0.3",
        "tippy.js": "^6.3.7"
    },
    "devDependencies": {
        "@babel/cli": "^7.28.0",
        "@babel/core": "^7.28.0",
        "@babel/node": "^7.28.0",
        "@babel/preset-env": "^7.28.0",
        "babel-preset-es2015": "^6.24.1",
        "browser-resolve": "^2.0.0",
        "browserify": "^17.0.1",
        "esmify": "^2.1.1",
        "fs-extra": "^11.3.0",
        "nexe": "^5.0.0-beta.4",
        "prettier": "3.6.2"
    }
}

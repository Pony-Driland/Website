{
    "engines": {
        "node": ">=20.0.0"
    },
    "scripts": {
        "start": "node ./build/",
        "start:tiny-chat": "npx babel-node server/tiny-chat/index.js",
        "full-build:tiny-chat": "yarn && yarn build:tiny-chat",
        "build:tiny-chat": "nexe index.js server/tiny-chat/index.js -o tiny-chat --build",
        "build:windows:tiny-chat": "nexe index.js server/tiny-chat/index.js -o tiny-chat --build --target windows-x64",
        "build:linux:tiny-chat": "nexe index.js server/tiny-chat/index.js -o tiny-chat --build --target linux-x64",
        "build:mac:tiny-chat": "nexe index.js server/tiny-chat/index.js -o tiny-chat --build --target macos-x64",
        "example:tiny-chat-timedmap": "npx babel-node server/tiny-chat/examples/TimedMap.js",
        "install": "yarn build-bundle",
        "build-bundle": "browserify build/bundle/require -o docs/bundle.js && browserify build/bundle/import -p esmify > docs/bundle2.js && npx babel-node build/bundle/css && npx babel-node build/bundle/files",
        "fix:prettier": "prettier --write ./docs/js/chapter_manager/* && prettier --write ./docs/js/files/* && prettier --write ./docs/js/ai/* && prettier --write ./docs/js/pwa/* && prettier --write ./docs/js/crypto/* && prettier --write ./server/tiny-chat/*",
        "update-base64": "node ./build/base64",
        "update-chapter": "node ./build/chapter",
        "update-characters": "node ./build/characters",
        "update-sitemap": "node ./build/sitemap",
        "deploy-keybase": "node ./build/deploy-keybase"
    },
    "author": {
        "name": "JasminDreasond (Yasmin Seidel)",
        "email": "tiny@puddy.club",
        "url": "https://github.com/JasminDreasond"
    },
    "dependencies": {
        "@cryptofonts/cryptofont": "^1.3.5",
        "@fortawesome/fontawesome-free": "^6.7.2",
        "@popperjs/core": "^2.11.8",
        "@walletconnect/client": "^1.8.0",
        "@walletconnect/qrcode-modal": "^1.8.0",
        "@walletconnect/web3-provider": "^1.8.0",
        "bignumber.js": "^9.1.2",
        "bootstrap": "^5.3.3",
        "buffer": "^6.0.3",
        "clone": "^2.1.2",
        "ethers": "^5.8.0",
        "events": "^3.3.0",
        "exifreader": "^4.26.0",
        "express": "^4.20.0",
        "file-saver": "^2.0.5",
        "for-promise": "^1.4.3",
        "gasparesganga-jquery-loading-overlay": "^2.1.7",
        "glob": "^11.0.1",
        "ini": "^5.0.0",
        "jquery": "^3.7.1",
        "js-base64": "^3.7.7",
        "js-loading-overlay": "^1.2.0",
        "jsonrepair": "^3.12.0",
        "marked": "^15.0.7",
        "md5": "^2.3.0",
        "moment": "^2.30.1",
        "node-fetch": "^3.2.0",
        "object-hash": "^3.0.0",
        "paginate-array": "^2.1.0",
        "pg": "^8.14.1",
        "photoswipe": "^5.4.4",
        "pizzicato": "^0.6.4",
        "qrcode": "^1.5.0",
        "sitemap": "^8.0.0",
        "socket.io": "^4.8.1",
        "socket.io-client": "^4.8.1",
        "sqlite": "^5.1.1",
        "sqlite3": "^5.1.7"
    },
    "devDependencies": {
        "@babel/cli": "^7.27.0",
        "@babel/core": "^7.26.9",
        "@babel/node": "^7.26.0",
        "@babel/preset-env": "^7.26.9",
        "babel-preset-es2015": "^6.24.1",
        "browser-resolve": "^2.0.0",
        "browserify": "^17.0.1",
        "esmify": "^2.1.1",
        "prettier": "3.5.3"
    }
}
